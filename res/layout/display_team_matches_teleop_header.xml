<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/display_team_matches_teleop_header"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:paddingBottom="5dp"
    android:paddingTop="15dp" >

    <TextView
        android:id="@+id/display_header_teleop_match"
        style="@style/DisplayHeader"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/match" >
    </TextView>
    
        <TextView
        android:id="@+id/display_header_teleop_low_basket"
        style="@style/DisplayHeader"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:text="@string/STR_Shots_Low" >
    </TextView>

    <TextView
        android:id="@+id/display_header_teleop_medium_basket"
        style="@style/DisplayHeader"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:text="@string/STR_Shots_Med" >
    </TextView>

    <TextView
        android:id="@+id/display_header_teleop_high_basket"
        style="@style/DisplayHeader"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:text="@string/STR_Shots_High" >
    </TextView>

</LinearLayout>